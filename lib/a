class Solution {
    public boolean search(int[] nums, int target) {
      int lastIndex = nums.length - 1;
      int firstIndex = 0;
      int left = firstIndex;
      int right = lastIndex;
        while(left <= right) {
          int mid = left + (right - left) / 2;
          if(nums[mid] == target) {
            return true;
          }
          if (nums[mid] == nums[left]) {
                left = left + 1;
                continue;
          }
          //
          if(nums[firstIndex] < nums[mid]) {
            if(nums[mid] > target && nums[firstIndex] <= target) {
                right = mid - 1;
            } else {
                left = mid + 1;
            }
          } else {
            if(nums[mid] < target && nums[lastIndex] >= target) {
                left = mid + 1;
            } else {
                right = mid - 1;
            }
          }
      }
      return false;
